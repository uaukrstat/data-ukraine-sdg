            "title": "Subject"
          },
          {
            "name": "Sex",
            "type": "string",
            "constraints": {
              "enum": [
                "women",
                "men",
                ""
              ]
            },
            "title": "Sex"
          },
          {
            "name": "Type of residence",
            "type": "string",
            "constraints": {
              "enum": [
                "urban",
                "rural",
                ""
              ]
            },
            "title": "Type of residence"
          },
          {
            "name": "School type",
            "type": "string",
            "constraints": {
              "enum": [
                "lyceum / gymnasium / specialized school",
                "secondary school / educational complex",
                "college / technical secondary school / vocational and technical educational institution",
                ""
              ]
            },
            "title": "by type of educational institution"
          },
          {
            "name": "SES",
            "type": "string",
            "constraints": {
              "enum": [
                "pupils with low socioeconomic status (25% of pupils with the lowest scores on the PISA Economic Socioeconomic Status Index (ESCS))",
                "pupils with high socioeconomic status (25% of pupils with the highest scores on the PISA Economic Socioeconomic Status Index (ESCS))",
                ""
              ]
            },
            "title": "by socioeconomic status"
          },
          {
            "name": "Value",
            "type": "number",
            "title": "Value"
          }
        ]
