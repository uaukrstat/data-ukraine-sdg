      "format": "csv",
      "hashing": "md5",
      "encoding": "utf-8",
      "schema": {
        "fields": [
          {
            "name": "Year",
            "type": "integer",
            "title": "Year"
          },
          {
            "name": "Subject",
            "type": "string",
            "constraints": {
              "enum": [
                "reading",
                "mathematics",
                "natural sciences",
                ""
              ]
